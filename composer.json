{
    "name": "cosmix/klitikizer",
    "type": "library",
    "description": "Klitikizer is a vocative case converter for Greek names",
    "keywords": [
        "greek",
        "language",
        "utility",
        "vocative case",
        "text"
    ],
    "time": "2018-08-21",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Dimosthenis Kaponis",
            "email": "dkaponis@gmail.com",
            "homepage": "https://www.cosmix.org"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-intl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7 || ^6.4 || ^7.0"
    },
    "autoload": {
        "psr-4": {
            "Cosmix\\Klitikizer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cosmix\\Klitikizer\\Tests\\": "tests/"
        }
    }
